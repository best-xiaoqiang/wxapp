page{
    padding: 20rpx;
    box-sizing: border-box;
}
.item{
    font-size: 32rpx;
    color: #222;
    color: #07c160;
    margin-bottom: 20rpx;
}
.item.fail{
    color: #999;
    color: rgb(246, 87, 85);
}
.input-wrap{
    display: flex;
    margin-bottom: 20rpx;
    padding: 10rpx 20rpx;
    border-radius: 14rpx;
    border: 1px #07c160 solid;
}
.input-wrap input{
    flex: 1;
}
.input-wrap checkbox{
    margin-left: 20rpx;
}
.btn{
    margin: 30rpx auto;
    width: 250rpx;
}
.desc{
    margin-top: 120rpx;
    text-align: left;
    width: 600rpx;
}
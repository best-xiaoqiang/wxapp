<open-data class="avatar" type="userAvatarUrl" open-gid=""></open-data>
<view class="title">
    <text>{{dateShow}}</text>
</view>
<view class="wrap" bind:longtap="edit">
    <block wx:if="{{list && list.length}}">
        <view class="item {{item.success ? 'success' : ''}}" wx:for="{{list}}" wx:key="{{index}}">
            <view class="content">{{item.content}}</view>
            <icon type="{{item.success ? 'success_no_circle' : 'warn'}}" color="white"></icon>
        </view>
    </block>
    <block wx:else>
        <view class="none">清单为空，长按编辑</view>
    </block>
</view>
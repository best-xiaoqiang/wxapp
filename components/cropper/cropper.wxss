.cropper-comp-wrap{
  position: fixed;
  z-index: 100;
  background-color: #2c2c2c;
}
view {
  background-color: #2c2c2c;
}

.cropper-canvas-wrap {
  width: 100%;
  padding: 20rpx; /* 修改时要相应修改js文件中的canvasW的40 */
  box-sizing: border-box;
  z-index: 1;
}

.cropper-tip {
  font-size: 28rpx;
  color: #fff;
  text-align: center;
}

.cropper-btn-wrap {
  width: 100%;
  height: 88rpx;
  padding: 0 20rpx;
  box-sizing: border-box;
  margin: 60rpx 0;
}

.cropper-btn {
  width: 100%;
  height: 100%;
  background: #ffda44;
  border-radius: 10rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}

.cropper-btn text {
  font-size: 32rpx;
  color: #333;
}